{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["es6", "DOM", "ESNext"],
    "module": "commonjs",
    "baseUrl": "./",
    "types": ["cypress", "node", "cypress-axe"],
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["."]
}